{
  "name": "vue-ssr-hello-world",
  "description": "Vue SSR Experiments",
  "author": "Pau Sanchez",
  "private": true,
  "scripts": {
    "dev": "node server",
    "start": "cross-env NODE_ENV=production node server",
    "stats": "cross-env webpack --mode production --config build/webpack.client.config.js --profile --json > stats.json",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env webpack --mode production --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env webpack --mode production --config build/webpack.server.config.js --progress --hide-modules",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cross-env": "^5.2.0",
    "es6-promise": "^4.2.6",
    "express": "^4.16.4",
    "lru-cache": "^5.1.1",
    "route-cache": "0.4.4",
    "serve-favicon": "^2.5.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "chokidar": "^3.0.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mini-css-extract-plugin": "^0.6.0",
    "npm-check-updates": "^3.1.8",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "rimraf": "^2.6.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.4",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
